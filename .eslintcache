 description?: string;\n  id: string;\n}\n\nexport function useDesigner(){\n  const firestore = useFirestore()\n  \n  const storageRef = useStorage().ref();\n\n  const getPosts = (userId: string)=>new Promise<Array<object>>((resolve, reject) => {\n    firestore.collection('Posts').where('userId', '==', userId)\n    .get()\n    .then((res) => {\n      if(res.empty){\n        return reject('No matching entries found')\n      }\n      const docArray:Array<object> = [];\n      let count = 0;\n      res.forEach((doc) => {\n        storageRef.child(doc.data().imageIdentifier + '.jpg').getDownloadURL()\n        .then((url)=>{\n          count++;\n          docArray.push({data: doc.data(), imageUrl: url, id:doc.id});\n          if(count == res.size){\n            resolve(docArray);\n          }\n        }).catch(reject);\n      });\n    })\n    .catch(reject);\n  });\n\n  const getDesignerInfo = (designerId: string)=>new Promise<DesignerData>((resolve, reject) => {\n    firestore.collection('Designers').doc(designerId)\n    .get()\n    .then((doc) => {\n      if(doc.exists){\n        const data = doc.data()\n        if(data && data.name){\n          data.id = doc.id;\n          resolve(data as DesignerData);\n        }else{\n          reject('empty or mismatched data')\n        }\n        reject('document does not exist')\n      }\n      \n    })\n    .catch(reject);\n  });\n  \n  const getDesignerFromUserId = (userId:string)=>new Promise<DesignerData>((resolve, reject)=>{\n    firestore.collection('Designers').where('userId', '==', userId).get()\n    .then((res)=>{\n      if(res.empty){\n        return reject('No matching entries found')\n      }\n      \n      res.forEach((doc) => {\n        const data = doc.data();\n        if(data){\n          data.id = doc.id;\n          return resolve(data as DesignerData);\n        } else{\n          return reject('designer had incorrect data');\n        }\n      });\n    });\n  });\n\n  return {getPosts, getDesignerInfo, getDesignerFromUserId}\n}\n export type {DesignerData};","/Users/ryanmaciel/Desktop/swatchdeck-frontend/src/hooks/useFeed.ts",["112","113"],"/Users/ryanmaciel/Desktop/swatchdeck-frontend/src/hooks/useImages.ts",["114"],{"ruleId":"115","severity":1,"message":"116","line":19,"column":11,"nodeType":"117","endLine":19,"endColumn":75},{"ruleId":"118","severity":1,"message":"119","line":41,"column":6,"nodeType":"120","endLine":41,"endColumn":8,"suggestions":"121"},{"ruleId":"122","severity":1,"message":"123","line":22,"column":20,"nodeType":"124","messageId":"125","endLine":22,"endColumn":51,"fix":"126"},{"ruleId":"127","severity":1,"message":"128","line":37,"column":13,"nodeType":"129","messageId":"130","endLine":37,"endColumn":23},{"ruleId":"127","severity":1,"message":"131","line":43,"column":11,"nodeType":"129","messageId":"130","endLine":43,"endColumn":15},{"ruleId":"127","severity":1,"message":"132","line":48,"column":11,"nodeType":"129","messageId":"130","endLine":48,"endColumn":20},{"ruleId":"127","severity":1,"message":"133","line":49,"column":11,"nodeType":"129","messageId":"130","endLine":49,"endColumn":23},{"ruleId":"127","severity":1,"message":"134","line":51,"column":11,"nodeType":"129","messageId":"130","endLine":51,"endColumn":16},{"ruleId":"127","severity":1,"message":"128","line":53,"column":11,"nodeType":"129","messageId":"130","endLine":53,"endColumn":21},{"ruleId":"127","severity":1,"message":"135","line":5,"column":10,"nodeType":"129","messageId":"130","endLine":5,"endColumn":20},{"ruleId":"127","severity":1,"message":"136","line":5,"column":22,"nodeType":"129","messageId":"130","endLine":5,"endColumn":29},{"ruleId":"127","severity":1,"message":"137","line":5,"column":31,"nodeType":"129","messageId":"130","endLine":5,"endColumn":43},{"ruleId":"127","severity":1,"message":"138","line":7,"column":16,"nodeType":"129","messageId":"130","endLine":7,"endColumn":22},{"ruleId":"127","severity":1,"message":"139","line":29,"column":11,"nodeType":"129","messageId":"130","